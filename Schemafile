create_table "fukuros", force: :cascade do |t|
  t.integer  "user_id",    limit: 4,     null: false
  t.string   "title",      limit: 256,     null: false
  t.string   "salt",       limit: 16,     null: false
  t.string   "title_hash", limit: 64,     null: false
  t.datetime "created_at",               null: false
  t.datetime "updated_at",               null: false
end

add_index "fukuros", ["user_id"], name: "idx_user_id", using: :btree

create_table "users", force: :cascade do |t|
  t.string   "uuid",       limit: 36,     null: false
  t.datetime "created_at",                          null: false
  t.datetime "updated_at",                          null: false
end

add_index "users", ["uuid"], name: "idx_uuid", using: :btree, unique: true

create_table "media", force: :cascade do |t|
  t.integer  "fukuro_id",    limit: 4,     null: false
  t.string   "content_hash", limit: 64,     null: false
  t.datetime "created_at",               null: false
  t.datetime "updated_at",               null: false
end

add_index "media", ["fukuro_id"], name: "idx_fukuro_id", using: :btree

create_table "thumbnails", force: :cascade do |t|
  t.integer  "media_id",    limit: 4,     null: false
  t.string   "content_hash", limit: 64,     null: false
  t.datetime "created_at",               null: false
  t.datetime "updated_at",               null: false
end

add_index "thumbnails", ["media_id"], name: "idx_media_id", using: :btree
